{
  "schemaVersion": 1,
  "id": "armorposer",
  "version": "${version}",
  "name": "Armor Poser",
  "description": "A GUI to allow configuration of Armor Stand properties",
  "authors": [
    "Mrbysco",
    "ShyNieke"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/armor-poser",
    "sources": "https://github.com/Mrbysco/armor-poser",
    "issues": "https://github.com/Mrbysco/armor-poser/issues"
  },
  "license": "MIT",
  "icon": "assets/armorposer/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.mrbysco.armorposer.ArmorPoser"
    ],
    "client": [
      "com.mrbysco.armorposer.ArmorPoserClient"
    ],
    "modmenu": [
      "com.mrbysco.armorposer.config.PoserModMenuIntegration"
    ]
  },
  "mixins": [
